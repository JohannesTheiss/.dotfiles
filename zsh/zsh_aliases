# default 
alias v="nvim"
alias ls="exa"
alias grep="grep --color"
alias diff="diff --color"
alias cp="cp -i" # Confirm before overwriting something
alias http="http --style=fruity"
alias df='df -h' # Human-readable sizes
alias free='free -m' # Show sizes in MB
alias sz='source ~/.zshrc'
alias ll='exa -la'
alias removeSpace="for f in *; do mv "$f" `echo $f | tr ' ' '_'`; done"
alias open='xdg-open'
alias streamDir='python3 -m http.server' # stream the current dir to the local network

# tmux
alias startB="tmux new -s builds"

# VBox
alias startVM="VBoxManage startvm ubuntu"

# git
alias gl="git log --oneline --all --graph"
alias gac='git add . && git commit'
alias gacp='git add . && git commit && git push'

# docker
alias dli="docker images -a" # dli = docker list images
alias dlc="docker container ls -a"
alias dlv="docker volume ls"
alias dln="docker network ls"
alias dp="docker ps"
alias dpa="docker ps -a"
alias dcl="docker compose logs -f"

# C++
alias m="make -j17"
alias c="make clean"
alias cm="make clean && make -j17"
