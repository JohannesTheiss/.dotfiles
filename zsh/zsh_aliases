# python
alias py="python3"
alias ca="conda activate"
alias pyMath="conda activate math"

# java compile and run
alias jcr="javac *.java && java"

# custom
alias burp="java --add-opens=java.desktop/javax.swing=ALL-UNNAMED --add-opens=java.base/java.lang=ALL-UNNAMED -jar ~/apps/BurpSuite/burpsuite_community_v2022.11.3.jar"
alias secres="cat ~/fh/Semester8/Projekt/code/CyberDemon/build/test-results/AttackResults.json | jq"

